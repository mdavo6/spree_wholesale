<% content_for :page_title do %>
  <%= link_to Spree.t("Wholesalers"), spree.admin_wholesalers_path %> /
  <%= @wholesaler.company %>
<% end %>

<%= form_for [:admin, @wholesaler], :html => { :class => "wholesaler-form" } do |f| %>
  <%= render "spree/shared/error_messages", :target => @wholesaler %>
  <%= render "form", :form => f %>
  <%= render "spree/admin/shared/edit_resource_links" %>
<% end %>
